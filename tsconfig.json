{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"allowJs": true,
		"sourceMap": true,
		"outDir": "./dist",
		"removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": [
		"./lib",
		"./server",
	]
}
